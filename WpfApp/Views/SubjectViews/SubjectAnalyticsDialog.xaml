<Window x:Class="WpfApp.Views.SubjectViews.SubjectAnalyticsDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp.Views.SubjectViews"
        mc:Ignorable="d"
        Title="SubjectAnalyticsDialog" Height="400" Width="700">
    <Window.Background>
        <ImageBrush ImageSource="./../../Resources/main.jpg"/>
    </Window.Background>
    <Grid>
        <TextBlock Text="{Binding Path=Analyse}" Margin="40" FontSize="28"/>
    </Grid>
</Window>
